{
	"rust-analyzer.linkedProjects": [
		".\\api\\Cargo.toml",
		".\\desktop\\Cargo.toml",
		".\\mobile\\Cargo.toml",
		".\\ui\\Cargo.toml",
		".\\web\\Cargo.toml"
	],
	// Activate the `server` feature so cfg(feature = "server") code is not marked inactive
	"rust-analyzer.cargo.features": ["server"],
	"rust-analyzer.cargo.allFeatures": false,
	"tailwindCSS.experimental.classRegex": ["class: \"(.*)\""],
	"tailwindCSS.includeLanguages": {
		"rust": "html"
	},
	"files.associations": {
		"*.css": "tailwindcss"
	},
	"editor.quickSuggestions": {
		"strings": "on"
	}
}